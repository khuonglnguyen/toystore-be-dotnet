<div class="new">
    <div class="container">
        <div class="title-info wow fadeInUp animated" data-wow-delay=".5s">
            <h3 class="title" style="font-family:'UTM Nyala'">Sản Phẩm <span>Mới</span></h3>
            <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit curabitur </p>
        </div>
        <div class="new-info">
            @{
                int counter1 = 1;
                int counter2 = 1;
            }
            @foreach (var item in productsNew)
            {
                if (counter1 == 1)
                {
                    <div class="col-md-3 new-grid simpleCart_shelfItem wow flipInY animated" data-wow-delay=".5s">
                        <div class="new-top">
                            <a href="single.html"><img src="@Url.Content("~/Content/images/"+item.Image1)" style="height:250px;width:300px" class="img-responsive" alt="" /></a>
                            <div class="new-text">
                                <ul>
                                    <li><a href="@Url.Action("Details","Product",new { ID=item.ID})">Xem Chi Tiết </a></li>
                                    <li>@Ajax.ActionLink("Thêm Vào Giỏ", "AddItemCart", "Cart", new { ID = item.ID, @class = "item_add" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "DivCart", InsertionMode = InsertionMode.Replace }, new { @class = "AddItemCart" })</li>
                                </ul>
                            </div>
                        </div>
                        <div class="new-bottom">
                            <h5><a class="name" href="@Url.Action("Details","Product",new { ID=item.ID})" style="width: 200px;display: inline-block;white-space: nowrap;text-overflow: ellipsis;overflow: hidden;">@item.Name </a></h5>
                            <div class="rating">
                                <span class="on">☆</span>
                                <span class="on">☆</span>
